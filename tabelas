 CREATE TABLE ASSOCIADO (
  CNPJ_ASSOCIADO CHAR NOT NULL DEFAULT 14,
  ENDERECO_CEP INTEGER UNSIGNED NOT NULL,
  NOME_FANTASIA VARCHAR NULL DEFAULT 30,
  RAZAO_SOCIAL VARCHAR NOT NULL DEFAULT 30,
  NUMERO_ENDERECO VARCHAR NULL DEFAULT 8,
  TELEFONE_FIXO VARCHAR NULL DEFAULT 20,
  TELEFONE_CELULAR VARCHAR NULL DEFAULT 20,
  EMAIL VARCHAR NOT NULL DEFAULT 30,
  COMPLEMENTO VARCHAR NULL DEFAULT 20,
  TIPO_PRODUTO VARCHAR NULL DEFAULT 15,
  OBSERVACAO VARCHAR NULL DEFAULT 70,
  TIPO_VENDA VARCHAR NULL DEFAULT 10,
  ENDERECO_SITE VARCHAR NULL DEFAULT 30,
  PRIMARY KEY(CNPJ_ASSOCIADO),
  INDEX ASSOCIADO_FKIndex1(ENDERECO_CEP)
);

CREATE TABLE Cliente (
  CNPJ_CLIENTE CHAR() NOT NULL DEFAULT 14,
  COLABORADOR_CPF CHAR NOT NULL,
  ENDERECO_CEP INTEGER UNSIGNED NULL,
  NOME_FANTASIA VARCHAR NULL DEFAULT 40,
  RAZAO_SOCIAL VARCHAR NOT NULL DEFAULT 40,
  NUMERO_ENDRECO VARCHAR NULL DEFAULT 10,
  CPF_VENDEDOR_RESPONSAVEL VARCHAR NULL DEFAULT 25,
  DATA_PRIMEIRO_CONTATO DATE NULL DEFAULT 8,
  ETAPA_VENDA VARCHAR NULL DEFAULT 10,
  ID_PRODUTO INTEGER UNSIGNED NULL DEFAULT 30,
  ID_PERGUNTA INTEGER UNSIGNED NULL DEFAULT 15,
  PRIMARY KEY(CNPJ_CLIENTE),
  INDEX Cliente_FKIndex2(ENDERECO_CEP),
  INDEX Cliente_FKIndex2(COLABORADOR_CPF)
);

CREATE TABLE COLABORADOR (
  CPF CHAR NOT NULL DEFAULT 14,
  ENDERECO_CEP INTEGER UNSIGNED NOT NULL DEFAULT 8,
  RG VARCHAR NULL DEFAULT 12,
  NOME_COMPLETO VARCHAR NOT NULL DEFAULT 30,
  TELEFONE_CELULAR VARCHAR NULL DEFAULT 25,
  TELEFONE_FIXO VARCHAR NULL DEFAULT 25,
  EMAIL VARCHAR NOT NULL DEFAULT 40,
  NUMERO_ENDERECO VARCHAR NULL DEFAULT 10,
  ACESSO_SISTEMA VARCHAR NOT NULL DEFAULT 8,
  NACIONALIDADE VARCHAR NULL DEFAULT 10,
  DATA_NASCIMENTO DATE NULL,
  SENHA VARCHAR NOT NULL DEFAULT 8,
  PRIMARY KEY(CPF),
  INDEX COLABORADOR_FKIndex1(ENDERECO_CEP)
);

CREATE TABLE CONTATO_CLIENTE (
  ID_CONTATO INTEGER UNSIGNED NOT NULL DEFAULT 8 AUTO_INCREMENT,
  Cliente_CNPJ_CLIENTE CHAR() NOT NULL DEFAULT 14,
  NOME_COMPLETO VARCHAR NULL DEFAULT 40,
  CARGO VARCHAR NULL DEFAULT 15,
  EMAIL VARCHAR NOT NULL DEFAULT 20,
  TELEFONE_FIXO VARCHAR NULL DEFAULT 20,
  TELEFONE_CELULAR VARCHAR NULL DEFAULT 20,
  PRIMARY KEY(ID_CONTATO),
  INDEX CONTATO_CLIENTE_FKIndex1(Cliente_CNPJ_CLIENTE)
);

CREATE TABLE ENDERECO (
  CEP INTEGER UNSIGNED NOT NULL DEFAULT 8,
  LOGRADOURO VARCHAR NULL DEFAULT 15,
  BAIRRO VARCHAR NULL DEFAULT 15,
  CIDADE VARCHAR NULL DEFAULT 15,
  UF CHAR NULL DEFAULT 2,
  PRIMARY KEY(CEP)
);

CREATE TABLE PERGUNTA_CLIENTE (
  ID_PERGUNTA INTEGER UNSIGNED NOT NULL DEFAULT 8 AUTO_INCREMENT,
  Cliente_CNPJ_CLIENTE CHAR() NOT NULL DEFAULT 14,
  PERGUNTA1 VARCHAR NULL DEFAULT 15,
  PERGUNTA2 VARCHAR NULL DEFAULT 15,
  RESPOSTA1 VARCHAR NULL DEFAULT 15,
  RESPOSTA2 VARCHAR NULL DEFAULT 15,
  PRIMARY KEY(ID_PERGUNTA, Cliente_CNPJ_CLIENTE),
  INDEX PERGUNTA_CLIENTE_FKIndex1(Cliente_CNPJ_CLIENTE)
);

CREATE TABLE PRODUTO (
  idPRODUTO INTEGER UNSIGNED NOT NULL DEFAULT 10 AUTO_INCREMENT,
  ASSOCIADO_CNPJ_ASSOCIADO CHAR NOT NULL DEFAULT 14,
  DESCRICAO VARCHAR NULL DEFAULT 20,
  PRECO_UNITARIO INTEGER UNSIGNED NULL DEFAULT 8,
  PRIMARY KEY(idPRODUTO),
  INDEX PRODUTO_FKIndex1(ASSOCIADO_CNPJ_ASSOCIADO)
);
